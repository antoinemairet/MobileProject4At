<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobileproject4a">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".injection.EsieaApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MobileProject4A">
        <activity android:name=".presentation.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.createaccount.CreateAccountActivity"
            android:label="Creation of your account" />
        <activity
            android:name=".presentation.list.ListActivity"
             />
    </application>

</manifest>